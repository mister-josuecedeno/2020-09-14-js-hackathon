: border-box;\n  margin: 0;\n  padding: 0;\n  font-family: Helvetica, Arial, sans-serif;\n}\n\ndiv {\n  border: 1px solid gray;\n}\n\ni {\n  margin-right: 0.5em;\n}\n\n.divider {\n  font-size: 2em;\n  padding: 0 0.5em;\n  color: gray;\n}\n\n.wrapper {\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  grid-template-rows: [title] auto [tools] auto [board] 90vh [footer] auto;\n  border: 1px solid #00a2ed;\n}\n\n/* Title */\n.title {\n  grid-column: 1 / 4;\n  grid-row-start: title;\n  background: #00a2ed;\n  color: white;\n  padding: 0.75em;\n  border-radius: 5px 5px 0px 0px;\n}\n\n/* canvas */\n.board {\n  grid-column: 1 / 4;\n  grid-row-start: board;\n  background: gray;\n  padding: 0.5em;\n}\n\n.paper {\n  background: white;\n  height: 100%;\n  width: 100%;\n}\n\n/* Tools */\n.tools {\n  padding: 0.75em;\n  grid-column: 1 / 4;\n  grid-row: tools;\n  background: rgb(198, 198, 198);\n\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  gap: 5px;\n  flex-wrap: wrap;\n}\n\n/* THickness */\n.thickness {\n  border: 1px solid rgb(198, 198, 198);\n}\n\n/* buttons */\nbutton {\n  width: 2em;\n  height: 2em;\n  border: 0;\n  line-height: 2.5;\n  padding: 0 20px;\n  font-size: 1rem;\n  text-align: center;\n  color: #fff;\n  text-shadow: 1px 1px 1px #000;\n  border-radius: 10px;\n  background-image: linear-gradient(\n    to top left,\n    rgba(0, 0, 0, 0.2),\n    rgba(0, 0, 0, 0.2) 30%,\n    rgba(0, 0, 0, 0)\n  );\n  box-shadow: inset 2px 2px 3px rgba(255, 255, 255, 0.6),\n    inset -2px -2px 3px rgba(0, 0, 0, 0.6);\n}\n\nbutton:hover {\n  cursor: pointer;\n}\n\n/* Paints */\n\n#yellow {\n  background: #ffbb00;\n}\n\n#green {\n  background: #7cbb00;\n}\n\n#blue {\n  background: #00a1f1;\n}\n\n#red {\n  background: #f65314;\n}\n\n/* Eraser */\n\n#eraser {\n  cursor: pointer;\n}\n\n#eraser:hover {\n  color: #00a1f1;\n}\n\n@media screen and (max-width: 600px) {\n  /* Thickness */\n  .thickness {\n    flex-basis: 100%;\n  }\n\n  #divider1 {\n    opacity: 0;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"50cee70a2b903b07cd34dd61326de63f","cacheData":{"env":{}}}